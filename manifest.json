{
  "name": "",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Replace every image on a website with the click of a button! Icons made by https://www.flaticon.com/authors/freepik from Flaticon",
  "homepage_url": "https://theopensuite.com",
  "icons": {
    "16": "icon-transparent.png",
    "128": "icon-transparent.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "content_scripts": [
    {
    "matches": ["http://*/*", "https://*/*"],
    "js": ["cats.js","inject.js"]
    }
  ],
  "browser_action": {
    "default_title": ""
  },
  "permissions": [
    "activeTab",
    "storage",
    "tabs"
  ]
}
